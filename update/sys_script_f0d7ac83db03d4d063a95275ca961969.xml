        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	//variable glidedatetime  para obtener el año de la fecha aceptacion servicio
	var year= new GlideDateTime(current.u_slx_fecha_aceptacion_servicio);
	year.getYearNoTZ;
	//Año		
	
	
	var gr= new GlideRecord('x_hocsa_san_encuesta_de_proyecto');
	gr.addQuery('u_slx_entregable', current.sys_id);
	gr.query();
	
	if(gr.next())
		{
			gr.u_slx_ano=year;
			gr.update();
		}
	
	
	
})(current, previous);]]></script>
