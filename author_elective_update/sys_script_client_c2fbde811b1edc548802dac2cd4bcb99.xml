        <condition/>
        <description>pinta el campo isc de los colores correspondientes</description>
        <field>u_slx_isc</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>SLX ISC Encuesta de proyecto</name>
        <order/>
        <script><![CDATA[function onLoad() {
  //Declarar variables de promedio e isc
var prom=g_form.getValue('u_slx_promedio');
var isc = g_form.getElement('u_slx_isc');
//Dependiendo del valor del promedio, se pintara el color del campo isc
	if(prom){
	if(prom>=0 && prom<=4){
		isc.style.backgroundColor = "OrangeRed";
	}else if(prom==5 || prom==6){
		isc.style.backgroundColor = "yellow";
	}else if(prom>=7 && prom<=10){
		isc.style.backgroundColor = "GreenYellow";
	}else  {
		isc.style.backgroundColor = "white";
	}}else {
		isc.style.backgroundColor = "white";
	}
	
	
   
}]]></script>
